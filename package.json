{
  "name": "entropy-web",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "plop": "plop",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:e+": "turbo run dev --filter=entropy-plus",
    "dev:dsl": "turbo run dev --filter=docs"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.17.0"
}